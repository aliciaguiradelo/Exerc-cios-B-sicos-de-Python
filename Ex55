r = "S"
numeros = []

while(r=="S"):
    quant = int(input('Quantidade de números que serão digitados: '))
    while(quant < 0 or quant >=20):
        print('Opa! Não pode números negativos e quantidade deve ser menor que 20')
        quant = int(input('Quantidade de números que serão digitados: '))
    for i in range(0, quant, 1):
        num = int(input('Digite um número: '))
        numeros.append(num)

    posicao = int(input("Digite o valor para saber sua posição:"))

    for i in range(0,quant,1):
        if (numeros[i]== posicao):
            posi = i
        if posi != -1:
            print("Esse número está na posição", posi)
        else:
            print("Valor não encontrado")
    r=input("Você deseja realizar uma nova execução?(S/N):").upper()
    while(r!="S" and r!="N"):
        r=input("Digite uma resposta válida:").upper()